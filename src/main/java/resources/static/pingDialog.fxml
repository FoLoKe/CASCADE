<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TitledPane contentDisplay="CENTER" prefHeight="135.0" prefWidth="372.0" text="Ping scan" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <TilePane alignment="TOP_CENTER" prefHeight="190.0" prefWidth="426.0">
         <children>
            <VBox prefHeight="91.0" prefWidth="349.0">
               <children>
                  <HBox prefHeight="60.0" prefWidth="410.0">
                     <children>
                        <VBox prefHeight="60.0" prefWidth="185.0">
                           <children>
                              <Label text="Network" />
                              <TextField fx:id="addressTextField" prefHeight="25.0" prefWidth="146.0" />
                           </children>
                           <padding>
                              <Insets right="10.0" />
                           </padding>
                        </VBox>
                        <VBox prefHeight="60.0" prefWidth="169.0">
                           <children>
                              <Label text="Mask" />
                              <TextField fx:id="maskTextField" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                  <HBox prefHeight="30.0" prefWidth="200.0">
                     <children>
                        <AnchorPane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" />
                           </children>
                        </AnchorPane>
                        <Button fx:id="okButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="133.0" text="Ok" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </TilePane>
   </content>
</TitledPane>
